<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
				
				<servlet>
		<servlet-name>ChangeRoomServlet</servlet-name>
		<servlet-class>igor.ChangeRoomServlet</servlet-class>
			</servlet>
	<servlet-mapping>
		<servlet-name>ChangeRoomServlet</servlet-name>
		<url-pattern>/changeroom</url-pattern>
			</servlet-mapping>
				
				
				
				<servlet>
		<servlet-name>MListServlet</servlet-name>
		<servlet-class>igor.MListServlet</servlet-class>
			</servlet>
	<servlet-mapping>
		<servlet-name>MListServlet</servlet-name>
		<url-pattern>/mlist</url-pattern>
			</servlet-mapping>
			
	<servlet>
		<servlet-name>GetMessageServlett</servlet-name>
		<servlet-class>igor.GetMessageServlett</servlet-class>
			</servlet>
	<servlet-mapping>
		<servlet-name>GetMessageServlett</servlet-name>
		<url-pattern>/message</url-pattern>
			</servlet-mapping>		
			
	<servlet>
	<servlet-name>LoginServlet</servlet-name>
		<servlet-class>igor.LoginServlet</servlet-class>
		</servlet>
		<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/access</url-pattern>
		</servlet-mapping>
		
<servlet>
<servlet-name>UserListServlet</servlet-name>
<servlet-class>igor.UserListServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>UserListServlet</servlet-name>
<url-pattern>/userlist</url-pattern>
</servlet-mapping>
	
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
